<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<layout class="ch.qos.logback.classic.PatternLayout">
			<Pattern>
				%d{HH:mm:ss.SSS} [%thread] %-5level %logger{128} - %msg%n
			</Pattern>
		</layout>
	</appender>
	
	<logger name="net.qmap" level="info" />
	
	<root level="info">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>